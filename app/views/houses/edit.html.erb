<% if @house.errors.any? %>
<ul>
  <% @house.errors.full_messages.each do |m| %>
  <%= m %>
  <% end %>
</ul>
<% end %>
<%= form_with(model: @house,local: true) do |f| %>
  <p>
    <%= f.label :Name %><br/>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :address %><br/>
    <%= f.text_field :address %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
